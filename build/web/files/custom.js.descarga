$(document).ready(function($){var products_carousel=new Swiper('.products-carousel',{slidesPerView:6,nextButton:'.swiper-button-next',prevButton:'.swiper-button-prev',slidesPerGroup:1,breakpoints:{320:{slidesPerView:1},800:{slidesPerView:2},1200:{slidesPerView:3},1400:{slidesPerView:4},1800:{slidesPerView:5}}});var products_column_carousel=new Swiper('.products-column-carousel',{slidesPerView:1,direction:'vertical',nextButton:'.swiper-button-next',prevButton:'.swiper-button-prev',slidesPerGroup:1,breakpoints:{320:{slidesPerView:1},800:{slidesPerView:1},1300:{slidesPerView:1}}});var testimonials=new Swiper('#testimonials',{slidesToShow:1,pagination:'.swiper-pagination',paginationClickable:true,slidesToScroll:1});$('.count').each(function(){$(this).prop('Counter',0).animate({Counter:$(this).text()},{duration:4000,easing:'swing',step:function(now){$(this).text(Math.ceil(now));}});});});